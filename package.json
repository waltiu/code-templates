{
  "name": "code-templates",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "instal": "pnpm install",
    "tsc": "vue-tsc -b",
    "lint:fix": "eslint src --fix",
    "lint": "eslint src --ignore-pattern .gitignore --fix",
    "build": "npm run tsc && vite build",
    "preinstall": "npx only-allow pnpm",
    "preview": "vite preview",
    "lint-staged": "lint-staged",
    "lint:staged": "eslint",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": "npm run lint:staged"
  },
  "dependencies": {
    "pinia": "^2.1.7",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@eslint/js": "^9.5.0",
    "@types/node": "^20.14.9",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "9.x",
    "eslint-plugin-vue": "^9.26.0",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.14.1",
    "vite": "^5.3.1",
    "vue-tsc": "^2.0.21"
  }
}
